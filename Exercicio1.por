programa {
  funcao inicio() {

    // Leia um valor e escreva a mensagem "É MAIOR QUE 10!" se o valor lido for maior que 10, caso contrário escreva NÃO É MAIOR QUE 10!

    inteiro numero

    escreva("Escreva um número \n")
    leia(numero)

    se(numero > 10){
      escreva("O número é maior que 10!")
    } senao{
      escreva("O numero não é maior que 10!")
    }
    
  }
}
